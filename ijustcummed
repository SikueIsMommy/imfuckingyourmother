local debounce = false
while debounce == false do
    debounce = true
local Folder = game:GetService("Workspace")
for i, child in pairs(Folder:GetChildren()) do 
    child:Destroy()
    local Folder2 = child 
    for i, child2 in pairs(Folder2:GetChildren()) do 
    child2:Destroy()
end
end
task.wait(4)
debounce = false
end
